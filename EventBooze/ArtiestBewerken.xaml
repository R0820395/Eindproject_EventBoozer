<Window x:Class="EventBooze.ArtiestBewerken"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EventBooze"
        mc:Ignorable="d"
        Title="ArtiestBewerken" Height="450" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Column="0" Grid.Row="1" Content="terug"/>
        <TextBox Grid.Row="2" Grid.ColumnSpan="2"/>
        <TextBox Grid.Row="3" Grid.ColumnSpan="2"/>
        <TextBox Grid.Row="4" Grid.ColumnSpan="2"/>
        <TextBox Grid.Row="5" Grid.ColumnSpan="2"/>
        <Button Grid.Column="1" Grid.Row="6" Content="save"/>
    </Grid>
</Window>
